<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="mill_relining_robot">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import mill_relining_robot urdf file -->
    <xacro:include filename="$(find ROS2_MRM)/urdf/mill_relining_robot.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="mill_relining_robot.ros2_control.xacro" />

</robot>
